<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.hello_ug">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DashBoard"
            android:theme="@style/AppTheme"/>
        <activity android:name=".Activity_Transportation"
            android:theme="@style/ThemeTransportation"/>
        <activity android:name=".Activity_Hotel"
            android:theme="@style/ThemeHotel"/>
        <activity android:name=".Activity_Mall"
            android:theme="@style/ThemeMall"/>
        <activity android:name=".Activity_Places"
            android:theme="@style/ThemePlace"></activity>
    </application>

</manifest>